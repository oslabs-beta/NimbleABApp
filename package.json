{
  "name": "NimbleAB",
  "description": "An IDE for AB Testing",
  "version": "1.0.0",
  "main": "app/electron/main.js",
  "author": "Team JAZ",
  "license": "MIT",
  "types": "./index.d.ts",
  "build": {
    "productName": "NimbleAB",
    "appId": "com.Nimble|electron.NimbleAB",
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "app/dist/**/*",
      "app/electron/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ]
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "audit-app": "npx electronegativity -i ./ -x LimitNavigationGlobalCheck,PermissionRequestHandlerGlobalCheck",
    "dev-server": "cross-env NODE_ENV=development webpack serve --config ./webpack.development.js > dev-scripts/webpack-dev-server.log 2> dev-scripts/webpack-dev-server-error.log",
    "dev": "concurrently --success first \"node dev-scripts/prepareDevServer.js\" \"node dev-scripts/launchDevServer.js\" -k",
    "prod-build": "cross-env NODE_ENV=production npx webpack --mode=production --config ./webpack.production.js",
    "prod": "npm run prod-build && electron .",
    "pack": "electron-builder --dir",
    "dist": "npm run test && npm run prod-build && electron-builder",
    "dist-mac": "npm run test && npm run prod-build && electron-builder --mac",
    "dist-linux": "npm run test && npm run prod-build && electron-builder --linux",
    "dist-windows": "npm run prod-build && electron-builder --windows",
    "dist-all": "npm run prod-build && electron-builder install-app-deps && electron-builder --mac --linux --windows",
    "test": "mocha"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@loadable/component": "^5.15.2",
    "@mui/material": "^5.14.6",
    "@reduxjs/toolkit": "^1.8.3",
    "@supabase/supabase-js": "^2.33.1",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.0.6",
    "axios": "^1.5.0",
    "easy-redux-undo": "^1.0.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.1.0",
    "glob": "^10.3.3",
    "install": "^0.13.0",
    "npm": "^9.8.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-first-history": "^5.1.1",
    "supabase": "^1.88.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@doyensec/electronegativity": "^1.9.1",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "daisyui": "^3.6.3",
    "electron": "^19.0.10",
    "electron-builder": "^23.1.0",
    "electron-debug": "^3.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "mocha": "^10.0.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.1",
    "spectron": "^19.0.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  }
}
